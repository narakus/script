[Unit]
Description=Model
After=syslog.target network.target remote-fs.target nss-lookup.target

[Service]
Type=forking
LimitCORE=infinity
LimitNOFILE=100000
LimitNPROC=100000
User=zeasn
Group=zeasn
PIDFile=/apps/Model/Model.pid
ExecStart=/apps/Model/bin/startup.sh                   
ExecReload=/bin/kill -s HUP $MAINPID
ExecStop=/bin/kill -s QUIT $MAINPID
PrivateTmp=true
User=zeasn
Group=zeasn
Restart=always

[Install]
WantedBy=multi-user.target


